<%= form_for @user do |f| %>

  <div>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>

  <div>
  <%= f.label :email %>
  <%= f.text_field :email %>
  </div>

  <div>
  <%= f.label :password %>
  <%= f.text_field :password %>
  </div>

  <div>
  <%= f.label :confirmpassword %>
  <%= f.text_field :confirmpassword %>
  </div>

  <div>
  <%= f.submit %>
  </div>

<% end %>
